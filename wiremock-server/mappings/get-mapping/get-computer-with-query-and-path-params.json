{
  "request": {
    "method": "GET",
    "urlPathPattern": "/api/computer/[0-9]+",
    "queryParameters": {
      "brand": {
        "equalTo": "Lenovo"
      },
      "model": {
        "equalTo": "ThinkPad"
      },
      "year": {
        "equalTo": "2025"
      },
      "price": {
        "equalTo": "2000.00"
      }
    }
  },
  "response": {
    "status": 200,
    "bodyFileName": "get/get-computer-with-query-and-path-params.json",
    "headers": {
      "Content-Type": "application/json"
    },
    "transformers": [
      "response-template"
    ]
  }
}
